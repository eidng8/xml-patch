<organization_settings>
    <name>NCI</name>
    <group_prefix>__nci-</group_prefix>
    <contributor_group_prefix>__nci__contributor-</contributor_group_prefix>
    <group>__nci</group>
    <admin_group>__nci-dba</admin_group>
    <document-status parent-path="prolog/metadata" status-node="othermeta" name-node="@name" value-node="@content" status-name-value="__status">
        <option name="Draft" value="draft" color="#4F1D1D"/>
        <option name="In Review" value="in_review" color="#3F3B3B"/>
        <option name="Approved" value="approved" color="#73DF61"/>
        <option name="Published" value="published" color="#2673CF"/>
        <option name="Localized" value="localized" color="#85008F"/>
    </document-status>
</organization_settings>
